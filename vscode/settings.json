{
  "workbench.activityBar.visible": false,
  "editor.minimap.enabled": false,
  "extensions.ignoreRecommendations": true,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDragAndDrop": false,
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 0,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.enablePreview": false,
  "editor.formatOnSave": true,
  "explorer.sortOrder": "type",
  "breadcrumbs.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.cursorStyle": "underline",
  "files.insertFinalNewline": true,
  "keyboard.dispatch": "keyCode",
  "explorer.autoReveal": false,
  "editor.renderControlCharacters": false,
  "python.formatting.autopep8Args": ["--max-line-length=200"],
  "editor.fontFamily": "'Fira Code', 'Source Code Pro', 'Terminus (TTF)', 'scientifica', 'monospace'",
  "editor.fontSize": 12,
  "terminal.integrated.fontFamily": "'Fira Code', 'Terminus (TTF)', 'monospace'",
  "terminal.integrated.fontSize": 12,
  "scm.diffDecorations": "none",
  "editor.glyphMargin": false,
  "workbench.statusBar.visible": true,
  "git.decorations.enabled": false,
  "workbench.tips.enabled": false,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-h>": false,
    "<C-w>": false,
    "<C-s>": false,
    "<C-x>": false,
    "<C-c>": false,
    "<C-v>": false
  },
  "editor.autoClosingOvertype": "never",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.clang_format_style": "{ BasedOnStyle: LLVM, AlignConsecutiveDeclarations: true, ColumnLimit: 1000, IndentWidth: 4, AllowShortCaseLabelsOnASingleLine: true, AlignConsecutiveAssignments: false, AlignConsecutiveDeclarations: false, AllowShortFunctionsOnASingleLine: false }",
  "explorer.compactFolders": false,
  "terminal.integrated.shell.linux": "/bin/zsh",
  "editor.renderWhitespace": "none",
  "terminal.integrated.commandsToSkipShell": [],
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.revealIfOpen": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.quickSuggestionsDelay": 200,
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  "workbench.colorTheme": "Monokai",
  "javascript.validate.enable": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "workbench.enableExperiments": false,
  "editor.wordWrap": "off",
  "workbench.editor.pinnedTabSizing": "shrink",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
